<ul class='crud-menu-btn list-inline float-right'>
  <li class="list-inline-item">
    <%= link_to edit_video_path(video) do %>
      <i class="fas fa-pen">編集</i>
    <% end %>
  </li>
  <li class="list-inline-item">
    <%= link_to video_path(video), method: :delete, data: { confirm: '削除してよろしいですか？' } do %>
      <i class="fas fa-trash">削除</i>
    <% end %>
  </li>
</ul>

